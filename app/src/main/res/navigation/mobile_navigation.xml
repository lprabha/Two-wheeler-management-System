<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home"
    tools:ignore="UnusedNavigation">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.two_wheeler_schedule_management_system.fragments.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/activity_fragment_home" />

    <fragment
        android:id="@+id/navigation_servicing"
        android:name="com.example.two_wheeler_schedule_management_system.fragments."
        android:label="@string/title_notification"
        tools:layout="@layout/activity_fragment_servicing" />

    <fragment
        android:id="@+id/navigation_booking"
        android:name="com.example.two_wheeler_schedule_management_system.fragments."
        android:label="@string/title_booking"
        tools:layout="@layout/activity_fragment_booking" />
    <fragment
        android:id="@+id/navigation_setting"
        android:name="com.example.two_wheeler_schedule_management_system.fragments."
        android:label="@string/title_setting"
        tools:layout="@layout/activity_fragment_setting"
        />
</navigation>